{% set f = simple_form(['admin',partner_url], [ 'class':'ajax_model_form']) %}
{{ f.input('name',['label': '名称']) }}
{{ f.select('type',['label': '类型', 'collection': PartnerUrls.TYPE]) }}
{{ f.select('platform',['label': '平台', 'collection': PartnerUrls.PLATFORM]) }}
{{ f.input('domain', ['label': '域名']) }}
{{ f.submit('保存') }}
<tr id="message_td" style="display: none;">
    <td colspan="4">
        <span class="error_reason" style="color: red;"></span>
    </td>
</tr>
{{ f.end }}