<form action="/admin/payment_channels/product_channels?payment_channel_id={{ payment_channel_id }}" method="post">
    {% for product_channel in product_channels %}
        <label>{{ product_channel.name }}</label>
        <input type="checkbox" name="product_channel_ids[]" value="{{ product_channel.id }}" {% if (checked[product_channel.id]) %}checked="checked"{% endif %}  "/>
    {% endfor %}
    <div class="form-group">
        <input type="submit" value="保存" class="btn btn-default btn-primary">
    </div>
</form>