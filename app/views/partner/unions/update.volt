<script>

    var opts = {
        data: {},
        methods: {}
    };

    var vm = XVue(opts);

    var $agree = $('#agreeScroll'), $Btn = $('.agree-btn'), $agreeShow = $('.agree-show'), $regShow = $('.reg-show'),
        $warn = $(".warn");

    $agree.scroll(function () {
        var sTop = $(this).scrollTop();
        var cH = $(this).innerHeight();
        var sH = $(this).prop('scrollHeight');
        if (sTop + cH >= sH) {
            $Btn.addClass("active")
        } else {
            $Btn.removeClass("active")
        }
        $agree.removeClass("warn");
        $warn.fadeOut()
    });

    $Btn.on("click", function () {
        if ($(this).hasClass("active")) {
            $agree.removeClass("warn");
            $agreeShow.fadeOut();
            tabs('.swiperTab > li', '.swiper-container', 'cur', 0);
        } else {
            $agree.addClass("warn");
            $warn.fadeIn();

        }

    });


    // 验证码倒计时
    var validCode = true;

    $('body').on('submit', '.union_info', function (event) {
        event.preventDefault();
        var self = $(this);
        var url = self.attr("action");

        self.ajaxSubmit({

            error: function (xhr, status, error) {
                alert('服务器错误 ' + error);
            },

            success: function (resp, status, xhr) {
                var error_code = resp.error_code;
                var error_reason = resp.error_reason;
                var error_url = resp.error_url;

                if (0 != error_code) {
                    alert(error_reason);
                    return
                }

                if (error_url) {
                    location.href = error_url;
                }
                // tabs('.swiperTab > li', '.swiper-container', 'cur', self.data('step'));
            }
        });

        return false;

    }).on('click', '#get_auth_code', function (event) {

        event.preventDefault();

        var mobile = $("#mobile").val();

        if (!$.isMobile(mobile)) {
            alert("请输入正确的手机号");
            return false;
        }

        var self = $(this);
        if (validCode) {

            $.authPost('/partner/unions/send_auth', {mobile: mobile}, function (resp) {

                var error_code = resp.error_code;
                var error_reason = resp.error_reason;

                if (0 != error_code) {
                    alert(error_reason);
                    return false;
                }

                var time = 60;
                self.html(time + "s后获取");


                validCode = false;
                var codeTimer = setInterval(function () {
                    time--;
                    self.html(time + "s后获取");
                    if (time == 0) {
                        clearInterval(codeTimer);
                        self.html("重新获取");
                        validCode = true;
                    }
                }, 1000)
            });
        }
    })

</script>
